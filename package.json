{
	"name": "@touchspot/prettier-config",
	"version": "3.0.1",
	"homepage": "https://github.com/touchspot/prettier-config#readme",
	"bugs": "https://github.com/touchspot/prettier-config/issues",
	"repository": {
		"type": "git",
		"url": "https://github.com/touchspot/prettier-config.git"
	},
	"license": "MIT",
	"author": {
		"name": "mozisan",
		"email": "mozisan@users.noreply.github.com"
	},
	"type": "module",
	"exports": {
		".": "./dist/default.js",
		"./tailwindcss": "./dist/tailwindcss.js"
	},
	"files": [
		"dist",
		"package.json"
	],
	"scripts": {
		"build": "pkgroll",
		"check:format": "prettier --check .",
		"check:lint": "eslint .",
		"check:type": "tsc --noEmit",
		"check:unused": "knip",
		"fix:format": "prettier --write --log-level=warn .",
		"fix:lint": "eslint --fix .",
		"prepare": "husky"
	},
	"prettier": "./src/default.ts",
	"dependencies": {
		"prettier-plugin-packagejson": "^2.5.19",
		"prettier-plugin-tailwindcss": "^0.7.1",
		"prettier-plugin-toml": "^2.0.6"
	},
	"devDependencies": {
		"@commitlint/cli": "^20.1.0",
		"@commitlint/config-conventional": "^20.0.0",
		"@commitlint/config-pnpm-scopes": "^20.1.0",
		"@touchspot/eslint-config": "^3.0.3",
		"@tsconfig/node22": "^22.0.2",
		"@tsconfig/strictest": "^2.0.6",
		"@types/node": "^22.18.12",
		"commitizen": "^4.3.1",
		"cz-git": "^1.12.0",
		"eslint": "^9.38.0",
		"husky": "^9.1.7",
		"knip": "^5.66.2",
		"lint-staged": "^16.2.6",
		"pkgroll": "^2.20.1",
		"prettier": "^3.6.2",
		"turbo": "^2.5.8",
		"typescript": "^5.9.3"
	},
	"peerDependencies": {
		"prettier": "^3.5.0"
	},
	"packageManager": "pnpm@10.19.0+sha512.c9fc7236e92adf5c8af42fd5bf1612df99c2ceb62f27047032f4720b33f8eacdde311865e91c411f2774f618d82f320808ecb51718bfa82c060c4ba7c76a32b8",
	"publishConfig": {
		"access": "public"
	}
}
