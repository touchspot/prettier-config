{
	"name": "@touchspot/prettier-config",
	"version": "2.0.1",
	"homepage": "https://github.com/touchspot/prettier-config#readme",
	"bugs": "https://github.com/touchspot/prettier-config/issues",
	"repository": {
		"type": "git",
		"url": "https://github.com/touchspot/prettier-config.git"
	},
	"license": "MIT",
	"author": {
		"name": "mozisan",
		"email": "mozisan@users.noreply.github.com"
	},
	"type": "module",
	"exports": {
		".": "./dist/default.js",
		"./tailwindcss": "./dist/tailwindcss.js"
	},
	"files": [
		"dist",
		"package.json"
	],
	"scripts": {
		"build": "pkgroll",
		"check:format": "prettier --check .",
		"check:lint": "eslint .",
		"check:type": "tsc --noEmit",
		"check:unused": "knip",
		"fix:format": "prettier --write --log-level=warn .",
		"fix:lint": "eslint --fix .",
		"prepare": "husky"
	},
	"prettier": "./src/default.ts",
	"dependencies": {
		"prettier-plugin-packagejson": "^2.5.19",
		"prettier-plugin-tailwindcss": "^0.6.14",
		"prettier-plugin-toml": "^2.0.6"
	},
	"devDependencies": {
		"@commitlint/cli": "^20.1.0",
		"@commitlint/config-conventional": "^20.0.0",
		"@commitlint/config-pnpm-scopes": "^20.1.0",
		"@touchspot/eslint-config": "^2.3.1",
		"@tsconfig/node22": "^22.0.2",
		"@tsconfig/strictest": "^2.0.6",
		"@types/node": "^22",
		"commitizen": "^4.3.1",
		"cz-git": "^1.12.0",
		"eslint": "^9.36.0",
		"husky": "^9.1.7",
		"knip": "^5.64.1",
		"lint-staged": "^16.2.3",
		"pkgroll": "^2.17.0",
		"prettier": "^3.6.2",
		"turbo": "^2.5.8",
		"typescript": "^5.9.3"
	},
	"peerDependencies": {
		"prettier": "^3.5.0"
	},
	"packageManager": "pnpm@10.12.4+sha512.5ea8b0deed94ed68691c9bad4c955492705c5eeb8a87ef86bc62c74a26b037b08ff9570f108b2e4dbd1dd1a9186fea925e527f141c648e85af45631074680184",
	"publishConfig": {
		"access": "public"
	}
}
